<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="300.0" prefWidth="300.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="gui.StaendeController">
	<children>
		<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
			minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0"
			prefWidth="300.0" VBox.vgrow="ALWAYS">
			<children>
				<Button fx:id="previous" layoutX="14.0" layoutY="54.0"
					mnemonicParsing="false" prefHeight="25.0" prefWidth="50.0"
					text="&lt;--" onAction="#previous" />
				<Button fx:id="next" layoutX="236.0" layoutY="54.0"
					mnemonicParsing="false" prefHeight="25.0" prefWidth="50.0"
					text="--&gt;" onAction="#next" />
				<Label fx:id="altKiloLabel" layoutX="64.0" layoutY="130.0"
					prefHeight="17.0" prefWidth="124.0" text="Alter Kilometerstand:" />
				<Label fx:id="neuKiloLabel" layoutX="64.0" layoutY="150.0"
					prefHeight="17.0" prefWidth="124.0" text="Neuer Kilometerstand:" />
				<Label fx:id="literLabel" layoutX="64.0" layoutY="170.0"
					prefHeight="17.0" prefWidth="124.0" text="Getankte Liter:" />
				<Label fx:id="verbrauchLabel" layoutX="64.0" layoutY="190.0"
					prefHeight="17.0" prefWidth="124.0" text="Verbrauch (l / 100km):" />
				<Label fx:id="altKilo" layoutX="211.0" layoutY="130.0"
					prefHeight="17.0" prefWidth="50.0" text="N/A" />
				<Label fx:id="neuKilo" layoutX="211.0" layoutY="150.0"
					prefHeight="17.0" prefWidth="50.0" text="N/A" />
				<Label fx:id="liter" layoutX="211.0" layoutY="170.0"
					prefHeight="17.0" prefWidth="50.0" text="N/A" />
				<Label fx:id="verbrauch" layoutX="211.0" layoutY="190.0"
					prefHeight="17.0" prefWidth="50.0" text="N/A" />
				<Button fx:id="fuckIt" layoutX="186.0" layoutY="222.0"
					mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0"
					text="Okay" onAction="#close" />
			</children>
		</AnchorPane>
	</children>
</VBox>
